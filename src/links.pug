include /mixins.pug
doctype html
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(http-equiv="X-UA-Compatible" content="IE=edge")
    meta(name="viewport" content="width=device-width, initial-scale=1.0")
    link(rel="icon" href="/logo16.png" type="image/png" sizes="16x16")
    title Links
    style
      include links.css
  body
    h1 Ruby's Links
    each p in g.links
      +link(p.link,p.title)